{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/app/(app)/product-details.jsx",
    "src/app/(app)/portfolio-details.jsx",
    "src/app/(app)/tranasaction-details.jsx",
    "src/app/(app)/fixed-income-withdrawal.jsx"
  ]
}
